---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
<channel>
  <title>{{ site.title }}</title>

  <link>{{site.url}}</link>

<language>en</language>

<copyright>{{site.copyright}}</copyright>

<itunes:subtitle>{{site.description}}</itunes:subtitle>

<itunes:author>{{site.author}}</itunes:author>

<itunes:keywords>{{site.keywords}}</itunes:keywords>

<itunes:summary>{{site.description}}</itunes:summary>

<description>{{site.description}}</description>

<itunes:owner>

<itunes:name>Beats, Rye &amp; Types</itunes:name>

<itunes:email>aaron+beatsryetypes@quirkey.com</itunes:email>

</itunes:owner>

<itunes:image href="http://www.beatsryetypes.com/images/cover-2000.png" />

<itunes:category text="Arts">
  <itunes:category text="Food" />
</itunes:category>
<itunes:category text="Technology" />

<itunes:new-feed-url>http://www.beatsryetypes.com/feed.xml</itunes:new-feed-url>

 {% for post in site.posts limit:10 %}
 
 <item>
 
 <title>{{ post.title }}</title>
 
 <itunes:author>{{ site.title }}</itunes:author>
 
 <itunes:subtitle>{{site.title}}: {{ post.title }}</itunes:subtitle>
 
 <itunes:summary>
 {{ post.content | split: '<!-- more -->' | first | xml_escape}}
 </itunes:summary>
 
 <itunes:image href="http://www.beatsryetypes.com/images/cover-2000.png" />
 
 <enclosure url="{{ post.link }}" length="{{ post.length }}" type="audio/mpeg" />
 
 <guid>{{ post.link }}</guid>
 
 <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S" }} GMT</pubDate>
 
 <itunes:duration>{{ post.duration }}</itunes:duration>
 
 <itunes:keywords>{{ post.keywords}},{{ site.keywords }}</itunes:keywords>
 
 </item>

 {% endfor %}

</channel>

</rss>
